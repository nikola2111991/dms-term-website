<script>

    var originalKeyword="";

    function showhelper() {
        if (document.getElementById("searchterm").value.length >= 0) {
            document.getElementById("mainCategory").style.visibility = 'visible';
            $('#sboard2').css('display', 'block');
            console.log('showboard2');
        }
    }

    function showhelper2() {
        if (document.getElementById("postnumber").value.length > 0) {
            document.getElementById("citySuggest").style.visibility = 'visible';
            $('#sboard3').css('display', 'block');
        }
    }

    if (typeof (document.getElementById('searchForm')) != 'undefined' && document.getElementById('searchForm') != null) {
        window.addEventListener('click', function (e) {
            if (!document.getElementById('searchForm').contains(e.target)) {
                $('.select-items').addClass('select-hide');
                $('#sboard2 ul').css('visibility', 'hidden');
                $('#sboard2').css('display', 'none');
                $('#sboard3 ul').css('visibility', 'hidden');
            }
        })
    }

    var intervalID = 0;

    function populateDirectoryCategories() {
        if (typeof document.getElementById("searchterm").value !== "undefined" && document.getElementById("searchterm").value != null) {
            clearTimeout(intervalID);
            intervalID = setTimeout(function () {
                var input = document.getElementById("searchterm").value;
                var helper = document.getElementById("mainCategory");
                var wraper = document.getElementById("srchwrpr");
                wraper.style.visibility = 'visible';
                var navigation = document.getElementById("navigation").value;
                if (input.length > 0) {
                    $('#searchterm').siblings('.clearer').show();
                    $.ajax({
                        type: 'POST',
                        url: "https://www.daibau.rs/suggestCategories",
                        data: {"searchterm": input, "navigation": navigation},
                        success: function (data) {
                            if (typeof (data) !== "undefined" && data.length > 0 && data != 'null') {
                                data = JSON.parse(data);
                                var result = "";
                                var preselectedCategory = 0;

                                if (typeof data.articles !== "undefined" && data.articles != null) {
                                    result += "<b>Magazin</b>";
                                    $.each(data.articles, function (index, value) {
                                        result += '<li><a href="https://www.daibau.rs/clanak/' + value.id + '/' + value.url + '">' + value.title + '</a></li>';
                                    });
                                }

                                if (typeof data.categories !== "undefined" && data.categories != null) {
                                    if (navigation == 'articles') result += "<hr><b>Kategorije</b>";
                                    $.each(data.categories, function (index, value) {
                                        //if (preselectedCategory == 0)
                                            //preselectedCategory = value.id;

                                        result += '<li onclick="populateCategory(' + value.id + ',\'' + value.category + '\');$(\'#postnumber\').focus();">' + value.category;
                                        if (navigation == 'inquiry') result+= '';
                                        if (navigation == 'directory') result+= '';
                                        if (navigation == 'articles') result+= '';
                                        if (navigation == 'calculator') result+= '';
                                        if (navigation == 'inquiry') {
                                            if (value.synonym != 0 && false) result += ' <font style="color:silver">(' + value.synonym + ')</font>';
                                            if (value.synonym2 != 0 && false) result += ' <font style="color:silver">(' + value.synonym2 + ')</font>';
                                        }
                                        result += '</li>';
                                    });

                                    if (result == "")
                                        result += '<li onclick="populateCategory(-1,\'Nema rezultata\');">Nema rezultata</li>';
                                }
                                if (typeof data.craftsmen !== "undefined" && data.craftsmen != null && data.craftsmen.length>0 && navigation != 'directorys') {
                                    result += "<div class='line' style='width:calc(100% - 30px);margin-left:20px;'></div><b>Preduzeća</b>";
                                    $.each(data.craftsmen, function (index, value) {
                                        result += '<li><a href="https://www.daibau.rs/' + value.url + '">' + value.name + '</a></li>';
                                    });
                                }

                                if (preselectedCategory > 0)
                                    $("#selectedCategory").val(preselectedCategory);

                                $('#mainCategory').empty();
                                $('#mainCategory').append(result);
                                helper.style.visibility = 'visible';
                                
                                showhelper();

                            }
                        }
                    });
                } else {
                    $.ajax({
                    type: 'POST',
                    url: "https://www.daibau.rs/getAllCategories",
                    data: { },
                    success: function(data)
                    {
                        if(typeof(data)!=="undefined" && data.length>0 && data!='null')
                        {   
                            console.log('hrtr d');
                            data = JSON.parse(data);
                            var result="";

                            if(typeof data.categories!=="undefined" && data.categories!=null)
                            {
                                
                                $.each(data.categories, function( index, value )
                                {
                                    result+='<li onclick="populateCategory('+value.id+',\''+value.category+'\');">'+value.category;
                                    result+='</li>';
                                });
                                                    
                                if(result=="")
                                    result+='<li onclick="populateCategory(-1,\'Nema rezultata\');">Nema rezultata</li>';
                            }
                        
                            $('#mainCategory').empty();
                            $('#mainCategory').append(result);
                            helper.style.visibility='visible';

                            showhelper();
                        }
                    }
                    });

                    $('#mainCategory').empty();
                    $('#mainCategory').append('<option selected="selected" value="-1">Nema rezultata</option>');
                    helper.style.visibility = 'hidden';
                }
            }, 500);
        }
    }

    function recordSearch(searchterm,originalKeyword,navigation,categoryId,cityId,source,target) {
        $.ajax({
            type: 'POST',
            url: "https://www.daibau.rs/admin/recordSearch",
            data: {"keyword":searchterm,"originalKeyword":originalKeyword,"navigation":navigation,"categoryId": categoryId, "cityId": cityId, "source": source, "target":target},
            success: function (data) {
                
            }
        });
    }

    function filterSearch() {
        var navigation = document.getElementById("navigation").value;
        var categoryId = document.getElementById("selectedCategory").value;
        var cityId = document.getElementById("cityId").value;
        var bezirkId = document.getElementById("bezirkId").value;

        var searchterm = document.getElementById("searchterm").value;
        var source=window.location.href;

        //directory
        if (navigation == "directory") {
            if ((cityId != -1 || bezirkId != -1) && categoryId != -1) {
                $.ajax({
                    type: 'POST',
                    url: "https://www.daibau.rs/directorylink",
                    data: {"category": categoryId, "cityId": cityId, "bezirkId": bezirkId},
                    success: function (data) {
                        data = JSON.parse(data);
                        recordSearch(searchterm,originalKeyword,navigation,categoryId,cityId,source,data);
                        window.location.replace(data);
                    }
                });
            }
        }

        //calculator or articles
        if (navigation == "calculator" || navigation == "articles" || navigation == "prodetails") {
            if (categoryId != -1) {
                $.ajax({
                    type: 'POST',
                    url: "https://www.daibau.rs/categorylink",
                    data: {"mainCategory": categoryId, "navigation": navigation},
                    success: function (data) {
                        data = JSON.parse(data);
                        recordSearch(searchterm,originalKeyword,navigation,categoryId,cityId,source,data);
                        window.location.replace(data);
                    }
                });

            } else {
                if(navigation=='articles') {
                    document.searchForm.action = "https://www.daibau.rs/magazin";
                    document.getElementById("searchForm").submit();
                } else if(navigation=='calculator') {
                    document.searchForm.action = "https://www.daibau.rs/cene";
                    document.getElementById("searchForm").submit();
                }
            }
        }

        //inquiry
        if (navigation == "inquiry") {
            if(searchterm.length>0) {
                if (categoryId >= -1) {
                    recordSearch(searchterm,originalKeyword,navigation,categoryId,cityId,source,"inquiryform");
                    document.getElementById("searchForm").submit();
                }
                else
                    recordSearch(searchterm,originalKeyword,navigation,categoryId,cityId,source,"inquiryform");
            } else {
                $("#searchterm").focus();
            }

        }


        if (cityId == -1 && bezirkId == -1 && navigation == "directory") {
            document.getElementById("postnumber").style.borderBottom = "1px solid var(--dborange-p)";
        } else document.getElementById("postnumber").style.border = "none";

        if (categoryId == -1) {
            document.getElementById("searchterm").style.borderBottom = "1px solid var(--dborange-p)";
            $("#searchterm").focus();
            populateDirectoryCategories();
        } else document.getElementById("searchterm").style.border = "none";
    }

    function populateCategory(categoryId, categoryName) {
        if (typeof categoryId !== "undefined" && categoryId > 0) {
            document.getElementById("selectedCategory").value = categoryId;
            originalKeyword=document.getElementById("searchterm").value;
            document.getElementById("searchterm").value = categoryName;
            document.getElementById("mainCategory").style.visibility = 'hidden';
            document.getElementById("searchterm").style.border = "none";
            $('#sboard2').css('display', 'none');
            filterSearch();

        } else {
            document.getElementById("searchterm").style.borderBottom = "1px solid var(--dborange-p)";
        }

    }

    var intervalID2 = 0; 

    function suggestCities(input,targetObject) {
        clearTimeout(intervalID2);
        targetObject = targetObject || 'citySuggest';

        intervalID2 = setTimeout(function () {
            var helper = document.getElementById(targetObject);
            document.getElementById("cityId").value = -1;
            if (typeof input !== "undefined" && input != null && input != "" && input.length > 1) {
                $('#postnumber').siblings('.clearer').show();

                $.ajax({
                    type: 'POST',
                    url: "https://www.daibau.rs/suggestCities",
                    data: {"searchstring": input},
                    success: function (data) {
                        data = JSON.parse(data);
                        var result = "";
                        var preselectedCity = 0;
                        var preselectedBezirk = 0;

                        if (data != null) {

                            if (typeof data.bezirken !== "undefined" && data.bezirken != null && data.bezirken.length > 0) {
                                document.getElementById("cityId").value = -1;
                                var first = true;
                                $.each(data.bezirken, function (index, value) {
                                    if (first) {
                                        first = !first;
                                        result += '<b>Bezirk</b><br />';
                                    }

                                    if (preselectedBezirk == 0)
                                        preselectedBezirk = value.id;

                                    result += '<li onclick="populateBezirk(' + value.id + ',\'' + value.name + '\',\'' + targetObject + '\');">' + value.name + '</li>';
                                });
                            }


                            if (typeof data.cities !== "undefined" && data.cities != null && data.cities.length > 0) {
                                var first = true;
                                $.each(data.cities, function (index, value) {
                                    if (first) {
                                        first = !first;
                                        document.getElementById("cityId").value = value.id;
                                        result += '<b>Kraj</b><br />';
                                    }

                                    if (preselectedBezirk == 0 && preselectedCity == 0)
                                        preselectedCity = value.id;

                                                                        result += '<li onclick="populateCity(' + value.id + ',\'' + value.city + '\',\'' + targetObject + '\');">' + value.city + '</li>';
                                                                    });
                            }

                            if (result == "")
                                result += '<li onclick="populateCity(-1,\'Nema rezultata\');">Nema rezultata</li>';

                            if (preselectedBezirk > 0)
                                $("#bezirkId").val(preselectedBezirk);
                            else if (preselectedCity > 0)
                                $("#cityId").val(preselectedCity);

                            $('#'+targetObject).empty();
                            $('#'+targetObject).append(result);
                            helper.style.visibility = 'visible';

                            if (document.getElementById('srchwrpr2'))
                            document.getElementById('srchwrpr2').style.visibility = 'visible';
                            
                            showhelper2();

                        }
                    }
                });
            } else {
                $('#'+targetObject)
                    .empty()
                    .append('<option selected="selected" value="-1">Nema rezultata</option>');
                helper.style.visibility = 'hidden';
            }
        }, 500);
    }

    function populateCity(cityId, cityName, targetObject) {
        if (typeof cityId !== "undefined" && cityId > 0) {
            document.getElementById("cityId").value = cityId;
            document.getElementById("bezirkId").value = -1;
            document.getElementById("postnumber").value = cityName;
            document.getElementById(targetObject).style.visibility = 'hidden';
            document.getElementById("postnumber").style.border = "1px solid silver";
            $("#postnumber2").css('border-color', 'silver');
            $('#sboard3').css('display', 'none');
            if(targetObject=='citySuggest') filterSearch();
            if(targetObject=='cSuggest') document.getElementById("postnumber2").value = cityName;
            if(targetObject=='clSuggest') $(".clInput").val(cityName);
        } else {
            document.getElementById(targetObject).style.visibility = 'hidden';
            document.getElementById("postnumber").style.border = "1px solid red";
        }
    }

    function populateBezirk(bezirkId, bezirkName, targetObject) {
        if (typeof bezirkId !== "undefined" && bezirkId > 0) {
            document.getElementById("cityId").value = -1;
            document.getElementById("bezirkId").value = bezirkId;
            document.getElementById("postnumber").value = bezirkName;
            document.getElementById(targetObject).style.visibility = 'hidden';
            document.getElementById("postnumber").style.border = "1px solid silver";
            $('#sboard3').css('display', 'none');
            if(targetObject=='citySuggest') filterSearch();
            if(targetObject=='cSuggest') document.getElementById("postnumber2").value = bezirkName;
        } else {
            document.getElementById("citySuggest").style.visibility = 'hidden';
            document.getElementById("postnumber").style.border = "1px solid red";
        }
    }

</script>

