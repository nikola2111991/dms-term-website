                        } else if (fileType == 'docs') {
                            prodetailFilesHtml = prodetailFilesHtml.replace('#{prodetail_file_translation}', 'Preuzmi tehničku dokumentaciju');
                        }

                        $('.prodetail-' + prodetailId + ' .'+fileType+'-details').show();
                        $('.prodetail-' + prodetailId + ' .'+fileType+'-details').append(prodetailFilesHtml);
