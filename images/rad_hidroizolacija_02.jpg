    }

    function subscribeNews(hash,type) {
        if (typeof hash !== "undefined" && hash != null && hash.length > 0) {
            $.ajax({
                type: 'POST',
                url: "https://www.daibau.rs/news/subscriptions",
                data: {"hash": hash, "type": type},
                async: false,
                success: function (data) {
                    if (typeof data !== "undefined" && data != null && data) {
                        //data = JSON.parse(data);
                        if (typeof data !== "undefined" && data != null && data) {
                            if (data.success) {
                                if(data.action=='subscribed') {
                                    var result = "Hvala Vam za prijavu i dobrodošli među čitaoce <b>magazina Daibau.rs</b>.";
                                } else if(data.action=='unsubscribed') {
                                    result = '<b>Odjavljeni</b> ste od primanja magazina Daibau.rs. Želite li da se ponovo prijavite? <a href="javascript:void(0);" onclick="subscribeNews(\'' + hash + '\',\'' + type + '\');" style="color:var(--dborange-p);">Prijava</a>';
                                } else if(data.action=='verifyemail') {
                                    result = 'Hvala Vam za prijavu i dobrodošli među čitaoce <b>magazina Daibau.rs</b>. Da biste završili prijavu, molimo vas da kliknete na link u imejlu koji smo vam poslali.';
                                } else
                                    result = 'Greška, ne postoji pretplata ili nemate pravo na uređivanje.';
                            } else {
                                result = 'Greška, ne postoji pretplata ili nemate pravo na uređivanje.';
                            }
                        } else {
                            result = 'Greška, ne postoji pretplata ili nemate pravo na uređivanje.';
                        }
                        document.getElementById("newsletterContent").innerHTML = result;
                    }
                },
                error: function () {
                    result = 'Greška, ne postoji pretplata ili nemate pravo na uređivanje.';
                    document.getElementById("newsletterContent").innerHTML = result;
                }
            });
        }
    }

    function getBrowserFingerprint(callback) {
        try {
            return Fingerprint2.get({}, function (components) {
                var values = components.map(function (component) {
                    return component.value
                });
                callback(Fingerprint2.x64hash128(values.join(''), 31));
            })
        } catch (err) {
            // Nothing to do here, we cannot get browser fingerprint.
            callback(false);
        }
    }

