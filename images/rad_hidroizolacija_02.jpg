    <script>
function setCookie(name, value, days) {
    const date = new Date();
    const expirationTime = date.getTime() + (days * 24 * 60 * 60 * 1000);
    date.setTime(expirationTime); // Set expiration time

    const expires = `expires=${date.toUTCString()}`;
    const sameSite = `SameSite=Lax`; // For cross-origin compliance
    const secure = window.location.protocol === 'https:' ? `Secure` : ''; // Add Secure for HTTPS

    //console.log(`Setting cookie: ${name}, expires on: ${date.toUTCString()}`);
    document.cookie = `${name}=${encodeURIComponent(value)}; ${expires}; path=/; ${sameSite}; ${secure}`;
}

// Function to get a cookie by name
function getCookie(name) {
    const nameEQ = `${name}=`;
    const cookies = document.cookie.split(';');
    for (let i = 0; i < cookies.length; i++) {
        let cookie = cookies[i].trim();
        if (cookie.indexOf(nameEQ) === 0) {
            return decodeURIComponent(cookie.substring(nameEQ.length));
        }
    }
    return null;
}

// Function to get URL parameters
function getUrlParameter(name) {
    const regex = new RegExp(`[?&]${name}=([^&#]*)`);
    const results = regex.exec(window.location.href);
    return results ? decodeURIComponent(results[1]) : null;
}

function handleUtmParameters() {
    const utmParams = ['utm_source', 'utm_medium', 'utm_campaign', 'utm_term', 'utm_content', 'gad_source', 'gclid'];
    const attributionPeriodDays = 30; // Default attribution period (in days)

    // Get current timestamp
    const now = new Date().getTime();

    // Retrieve existing campaigns from the cookie
    let campaigns = getCookie('daibau_ct');
    campaigns = campaigns ? JSON.parse(campaigns) : [];

    // Filter out expired campaigns
    campaigns = campaigns.filter(campaign => {
        const timeElapsed = now - campaign.timestamp;
        return timeElapsed <= attributionPeriodDays * 24 * 60 * 60 * 1000; // Time in milliseconds
    });

    // Capture new UTM parameters
    const newCampaign = {};
    let isNew = false;

    utmParams.forEach(param => {
        const value = getUrlParameter(param);
        if (value) {
            newCampaign[param] = value;
            isNew = true;
        }
    });

    if (isNew) {
        newCampaign.timestamp = now;

        // Check if the campaign already exists
        const existingIndex = campaigns.findIndex(campaign =>
            utmParams.every(param => campaign[param] === newCampaign[param])
        );

        if (existingIndex !== -1) {
            // Update the timestamp of the existing campaign
            campaigns[existingIndex].timestamp = now;
        } else {
            // Add the new campaign
            campaigns.push(newCampaign);
        }
    }

    // Update the cookie with the combined campaigns and set expiration
    setCookie('daibau_ct', JSON.stringify(campaigns), attributionPeriodDays);
}

