        });
    }

    function resendPhoneVerificationCode(hash, userId, successCallback, errorCallback) {
        $('.phoneVerificationError').hide();

        var queryParams = "";

        if (hash && hash != "") {
            queryParams = 'hash=' + hash;
        }

        if (userId && userId != "") {
            queryParams = 'userId=' + userId;
        }

        $.ajax({
            type: 'POST',
            url: "https://www.daibau.rs/registracija/resendPhoneVerificationCode?" + queryParams,
            success: function (data) {
                if (successCallback) {
                    successCallback(data);
                }
            },
            error: function (request, status, error) {
                if (errorCallback) {
                    errorCallback(request);
                } else if (request.responseText) {
                    var jsonResult = JSON.parse(request.responseText);
                    if (jsonResult && jsonResult.code === 102) {
                        $('.verify-phone').hide();
                        $('.phoneVerificationLimitReached').show();
                    } else {
                        $('.verify-phone').hide();
                        $('.phoneVerificationError').show();
                    }
                }
            }
        });
    }

    const COOLDOWN_DURATION_MS = 300000; // 5 minutes in milliseconds
    const STORAGE_KEY = 'lastResendAttemptTime';

    function tryResendPhoneVerificationCode(hash) {

        if (hash && hash != "") {
            hash = hash;
        }
        else hash="";

        const lastAttemptTime = localStorage.getItem(STORAGE_KEY);
        const currentTime = new Date().getTime();

        // 1. Check if a cooldown is active
        if (lastAttemptTime) {
            const timeElapsed = currentTime - parseInt(lastAttemptTime, 10);
            
            if (timeElapsed < COOLDOWN_DURATION_MS) {
                const timeRemainingMS = COOLDOWN_DURATION_MS - timeElapsed;
                const minutes = Math.floor(timeRemainingMS / 60000);
                const seconds = Math.ceil((timeRemainingMS % 60000) / 1000);
                
                // Format the remaining time for the alert
                let timeMessage = '';
                if (minutes > 0) {
                    timeMessage += `${minutes}m`;
                }
                if (seconds > 0) {
                    if (minutes > 0) timeMessage += ' i ';
                    timeMessage += `${seconds}s`;
                }

                message = 'Ponekad primanje SMS koda može potrajati nekoliko minuta. Molimo sačekajte {{timeMessage}}, pre nego što zatražite novi kod.';
                message = message.replace('{{timeMessage}}',timeMessage);

                // Alert the user to wait
                alert(message);
                
                // Block the execution of the function
                return false;
            }
        }

        // 2. If no cooldown is active, proceed with the resend action
        
        // Store the current time as the new last attempt time
        localStorage.setItem(STORAGE_KEY, currentTime.toString());
        
        resendPhoneVerificationCode(hash); 
        alert('SMS kod poslat.');
        
        return false;
    }

    function formatDate(time) {
        var formattedDate = new Date(time * 1000);
        var d = formattedDate.getDate();
        var m = formattedDate.getMonth();
        m += 1;
        var y = formattedDate.getFullYear();

        var h = formattedDate.getHours();
        var min = formattedDate.getMinutes();
        var s = formattedDate.getSeconds();

        if (d < 10) {
            d = "0" + d;
        }

        if (m < 10) {
            m = "0" + m;
        }

        if (y < 10) {
            y = "0" + y;
        }

        if (h < 10) {
            h = "0" + h;
        }

        if (min < 10) {
            min = "0" + min;
        }

        if (s < 10) {
            s = "0" + s;
        }

        return d + "." + m + "." + y; //+ " " + h + ":" + min + ":" + s;
    }

    function formatDateTime(time) {
        if (!time || time === "undefined")
            return "";

        var formattedDate = new Date(time * 1000);
        var d = formattedDate.getDate();
        var m = formattedDate.getMonth();
        m += 1;
        var y = formattedDate.getFullYear();

        var h = formattedDate.getHours();
        var min = formattedDate.getMinutes();
        var s = formattedDate.getSeconds();

        if (d < 10) {
            d = "0" + d;
        }

        if (m < 10) {
            m = "0" + m;
        }

        if (y < 10) {
            y = "0" + y;
        }

        if (h < 10) {
            h = "0" + h;
        }

        if (min < 10) {
            min = "0" + min;
        }

        if (s < 10) {
            s = "0" + s;
        }

        return d + "." + m + "." + y + " " + h + ":" + min + ":" + s;
    }

